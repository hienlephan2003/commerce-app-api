  "dependencies": {
    "axios": "^1.5.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.1.0",
    "mongodb-memory-server": "^9.0.1",
    "mongoose": "^7.5.2",
    "mysql": "^2.18.1",
    "nodemon": "^3.0.1",
    "tslib": "~2.6"
  },
